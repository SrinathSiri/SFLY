<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="SFLY3ExtentReport">
    
    <listeners>
        <listener class-name="extentreport.ReportManager"></listener>
    </listeners>
    
    <test verbose="2" preserve-order="true" name="Sfly3TitleExtentReportDynamic">
        <parameter name="os" value="windows"></parameter>
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="extentreport.Sfly3Title"></class>
        </classes>
    </test>
</suite>