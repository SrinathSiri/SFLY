<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="SFLY3ExtentReport">
    
    <listeners>
        <listener class-name="com.sfly3.helper.ExtentReportManager"></listener>
    </listeners>
    
    <test verbose="2" preserve-order="true" name="TC_003_LoginSfly3DataProvider">
        <parameter name="os" value="windows"></parameter>
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="com.sfly3.scripts.TC_003_LoginSfly3DataProvider"></class>
        </classes>
    </test>
</suite>